{
  "name": "MarioJS",
  "version": "0.0.1",
  "description": "Mario using JS",
  "main": "server.js",
  "repository": "git@github.com:aaa-ncnu-ie/Login-2.0.git",
  "author": "Richi Jeffery <richi1717@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "lint": "eslint -c eslint.config.json . --ext .js",
    "test": "yarn test:client && yarn test:server && yarn lint",
    "test:coverage": "yarn test:client -- --coverage && yarn test:server -- --coverage",
    "travis:coverage": "yarn test:client -- --coverage --maxWorkers=4 && yarn test:server -- --coverage --maxWorkers=4",
    "test:client": "NODE_ENV=test jest -c client/jestClientConfig.json",
    "test:server": "NODE_ENV=test jest -c server/jestServerConfig.json",
    "tdd:client": "yarn test:client -- --watch",
    "tdd:server": "yarn test:server -- --watch",
    "presendCoverage": "lcov-result-merger './coverage/**/lcov.info' ./coverage/lcov.info",
    "sendCoverage": "CODECLIMATE_REPO_TOKEN= codeclimate-test-reporter < coverage/lcov.info",
    "webpack": "NODE_ENV='production' webpack -p --config webpack.production.config.js",
    "webpack:watch": "webpack --config webpack.config.js",
    "server": "NODE_ENV='production' node server/runServer",
    "server:watch": "NODE_ENV='development' nodemon --watch server/ server/runServer.js",
    "start": "serve ./public",
    "styles": "node-sass --include-path scss client/styles/main.scss public/styles.css",
    "styles:watch": "yarn styles -- --watch",
    "build": "yarn webpack && yarn styles",
    "prod": "NODE_ENV='production' yarn build && yarn server",
    "dev": "npm-run-all --parallel start webpack:watch styles:watch"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-preset-env": "^1.6.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "codeclimate-test-reporter": "^0.5.0",
    "css-loader": "^0.28.4",
    "enzyme": "^3.1.0",
    "enzyme-matchers": "^4.0.0",
    "enzyme-to-json": "^3.1.2",
    "eslint": "3",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "jest-enzyme": "^4.0.0",
    "lcov-result-merger": "^1.2.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "supertest": "^3.0.0",
    "url-loader": "^0.5.9",
    "webpack": "^3.3.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-hot-middleware": "^2.18.2"
  },
  "dependencies": {
    "babel-eslint": "^7.2.3",
    "babel-polyfill": "^6.23.0",
    "body-parser": "^1.17.2",
    "chalk": "^2.0.1",
    "classnames": "^2.2.5",
    "cookie-parser": "^1.4.3",
    "express": "^4.15.3",
    "fetch": "^1.1.0",
    "lodash.assign": "^4.2.0",
    "lodash.merge": "^4.6.0",
    "prop-types": "^15.5.10",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "request-promise": "^4.2.1",
    "serve": "^6.2.0",
    "url-search-params": "^0.9.0",
    "whatwg-fetch": "^2.0.3"
  }
}
